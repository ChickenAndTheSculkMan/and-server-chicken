    "dashesleft":{
        "type":"origins:resource",
        "min":0,
        "max":2,
        "start_value":2,
        "hud_render":{
            "should_render":false
        }
    },
    "getdashes":{
        "type":"origins:action_over_time",
        "interval":1,
        "entity_action":{
            "type":"origins:if_else",
            "condition":{
                "type": "origins:in_block",
                    "block_condition": {
                        "type":"origins:height",
                        "comparison":"<=",
                        "compare_to":63
                    }
            },
            "if_action":{
                "type":"origins:nothing"
            },
            "else_action":{
                "type":"origins:change_resource",
                "resource":"madelinetheclimber:resources_dashesleft",
                "operation":"set",
                "change":2
            }
        },
        "condition":{
            "type": "origins:block_collision",
            "offset_x": 0,
            "offset_y": -0.1,
            "offset_z": 0
        }
    },
    "limitdashes":{
        "type":"origins:action_over_time",
        "interval":1,
        "entity_action":{
            "type":"origins:change_resource",
            "resource":"madelinetheclimber:resources_dashesleft",
            "operation":"set",
            "change":1
        },
        "condition":{
            "type":"origins:and",
            "conditions":[
                {
                    "type":"origins:resource",
                    "resource":"madelinetheclimber:resources_shouldlimitdash",
                    "comparison":"==",
                    "compare_to":1
                },
                {
                    "type":"origins:resource",
                    "resource":"madelinetheclimber:resources_dashesleft",
                    "comparison":">",
                    "compare_to":1
                }
            ]
        }
    },
