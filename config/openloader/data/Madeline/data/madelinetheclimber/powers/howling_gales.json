{
    "name":"Howling Gales",
    "description":"Each morning there's a chance for wind to start blowing on high altitudes.",
    "type":"origins:multiple",
    "choosewind":{
        "type":"origins:action_over_time",
        "interval":1,
        "condition":{
            "type":"origins:and",
            "conditions":[
                {
                    "type":"origins:time_of_day",
                    "comparison":">=",
                    "compare_to":0
                },
                {
                    "type":"origins:time_of_day",
                    "comparison":"<",
                    "compare_to":100
                }
            ]
        },
        "rising_action":{
            "type":"origins:and",
            "actions":[
                {
                    "type":"origins:change_resource",
                    "resource":"madelinetheclimber:resources_todayswind",
                    "operation":"set",
                    "change":0
                },
                {
                    "type":"origins:if_else",
                    "condition":{
                        "type":"origins:chance",
                        "chance":0.25
                    },
                    "if_action":{
                        "type":"origins:change_resource",
                        "resource":"madelinetheclimber:resources_strongwind",
                        "operation":"set",
                        "change":1
                    },
                    "else_action":{
                        "type":"origins:change_resource",
                        "resource":"madelinetheclimber:resources_strongwind",
                        "operation":"set",
                        "change":0
                    }
                },
                {
                    "type":"origins:chance",
                    "action":{
                        "type":"origins:choice",
                        "actions":[
                            {
                                "element":{
                                    "type":"origins:change_resource",
                                    "resource":"madelinetheclimber:resources_todayswind",
                                    "operation":"set",
                                    "change":1
                                },
                                "weight":10
                            },
                            {
                                "element":{
                                    "type":"origins:change_resource",
                                    "resource":"madelinetheclimber:resources_todayswind",
                                    "operation":"set",
                                    "change":2
                                },
                                "weight":10
                            },
                            {
                                "element":{
                                    "type":"origins:change_resource",
                                    "resource":"madelinetheclimber:resources_todayswind",
                                    "operation":"set",
                                    "change":3
                                },
                                "weight":10
                            },
                            {
                                "element":{
                                    "type":"origins:change_resource",
                                    "resource":"madelinetheclimber:resources_todayswind",
                                    "operation":"set",
                                    "change":4
                                },
                                "weight":10
                            },
                            {
                                "element":{
                                    "type":"origins:change_resource",
                                    "resource":"madelinetheclimber:resources_todayswind",
                                    "operation":"set",
                                    "change":5
                                },
                                "weight":10
                            },
                            {
                                "element":{
                                    "type":"origins:change_resource",
                                    "resource":"madelinetheclimber:resources_todayswind",
                                    "operation":"set",
                                    "change":6
                                },
                                "weight":10
                            }
                        ]
                    },
                    "chance":0.5
                },
                {
                    "type":"origins:if_else_list",
                    "actions":[
                        {
                            "condition":{
                                "type":"origins:resource",
                                "resource":"madelinetheclimber:resources_todayswind",
                                "comparison":"==",
                                "compare_to":0
                            },
                            "action":{
                                "type":"origins:execute_command",
                                "command":"title @s actionbar \"today's wind: [none]\""
                            }
                        },
                        {
                            "condition":{
                                "type":"origins:resource",
                                "resource":"madelinetheclimber:resources_todayswind",
                                "comparison":"==",
                                "compare_to":1
                            },
                            "action":{
                                "type":"origins:execute_command",
                                "command":"title @s actionbar \"today's wind: [blowing east]\""
                            }
                        },
                        {
                            "condition":{
                                "type":"origins:resource",
                                "resource":"madelinetheclimber:resources_todayswind",
                                "comparison":"==",
                                "compare_to":2
                            },
                            "action":{
                                "type":"origins:execute_command",
                                "command":"title @s actionbar \"today's wind: [blowing west]\""
                            }
                        },
                        {
                            "condition":{
                                "type":"origins:resource",
                                "resource":"madelinetheclimber:resources_todayswind",
                                "comparison":"==",
                                "compare_to":3
                            },
                            "action":{
                                "type":"origins:execute_command",
                                "command":"title @s actionbar \"today's wind: [blowing south]\""
                            }
                        },
                        {
                            "condition":{
                                "type":"origins:resource",
                                "resource":"madelinetheclimber:resources_todayswind",
                                "comparison":"==",
                                "compare_to":4
                            },
                            "action":{
                                "type":"origins:execute_command",
                                "command":"title @s actionbar \"today's wind: [blowing north]\""
                            }
                        },
                        {
                            "condition":{
                                "type":"origins:resource",
                                "resource":"madelinetheclimber:resources_todayswind",
                                "comparison":"==",
                                "compare_to":5
                            },
                            "action":{
                                "type":"origins:execute_command",
                                "command":"title @s actionbar \"today's wind: [blowing up]\""
                            }
                        },
                        {
                            "condition":{
                                "type":"origins:resource",
                                "resource":"madelinetheclimber:resources_todayswind",
                                "comparison":"==",
                                "compare_to":6
                            },
                            "action":{
                                "type":"origins:execute_command",
                                "command":"title @s actionbar \"today's wind: [blowing down]\""
                            }
                        }
                    ]
                }
            ]
        }
    },
    "windworlddirections":{
        "type":"origins:action_over_time",
        "interval":1,
        "entity_action":{
            "type":"origins:if_else_list",
            "actions":[
                {
                    "condition":{
                        "type":"origins:resource",
                        "resource":"madelinetheclimber:resources_todayswind",
                        "comparison":"==",
                        "compare_to":1
                    },
                    "action":{
                        "type":"origins:if_else",
                        "condition":{
                            "type":"origins:resource",
                            "resource":"madelinetheclimber:resources_strongwind",
                            "comparison":"==",
                            "compare_to":1
                        },
                        "if_action":{
                            "type":"origins:add_velocity",
                            "x":0.05
                        },
                        "else_action":{
                            "type":"origins:add_velocity",
                            "x":0.025
                        }
                    }
                },
                {
                    "condition":{
                        "type":"origins:resource",
                        "resource":"madelinetheclimber:resources_todayswind",
                        "comparison":"==",
                        "compare_to":2
                    },
                    "action":{
                        "type":"origins:if_else",
                        "condition":{
                            "type":"origins:resource",
                            "resource":"madelinetheclimber:resources_strongwind",
                            "comparison":"==",
                            "compare_to":1
                        },
                        "if_action":{
                            "type":"origins:add_velocity",
                            "x":-0.05
                        },
                        "else_action":{
                            "type":"origins:add_velocity",
                            "x":-0.025
                        }
                    }
                },
                {
                    "condition":{
                        "type":"origins:resource",
                        "resource":"madelinetheclimber:resources_todayswind",
                        "comparison":"==",
                        "compare_to":3
                    },
                    "action":{
                        "type":"origins:if_else",
                        "condition":{
                            "type":"origins:resource",
                            "resource":"madelinetheclimber:resources_strongwind",
                            "comparison":"==",
                            "compare_to":1
                        },
                        "if_action":{
                            "type":"origins:add_velocity",
                            "z":0.05
                        },
                        "else_action":{
                            "type":"origins:add_velocity",
                            "z":0.025
                        }
                    }
                },
                {
                    "condition":{
                        "type":"origins:resource",
                        "resource":"madelinetheclimber:resources_todayswind",
                        "comparison":"==",
                        "compare_to":4
                    },
                    "action":{
                        "type":"origins:if_else",
                        "condition":{
                            "type":"origins:resource",
                            "resource":"madelinetheclimber:resources_strongwind",
                            "comparison":"==",
                            "compare_to":1
                        },
                        "if_action":{
                            "type":"origins:add_velocity",
                            "z":-0.05
                        },
                        "else_action":{
                            "type":"origins:add_velocity",
                            "z":-0.025
                        }
                    }
                }
            ]
        },
        "condition":{
            "type":"origins:and",
            "conditions":[
                {
                    "type": "origins:in_block",
                    "block_condition": {
                        "type":"origins:height",
                        "comparison":">=",
                        "compare_to":120
                    }
                },
                {
                    "type": "origins:exposed_to_sky"
                }
            ]
        }
    },
